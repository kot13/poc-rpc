{
  "title": "Получение токена",
  "description": "Метод для получения токена",
  "needAuth": false,
  "needCache": false,
  "success": {
    "request": {
      "params": {
        "phone": {
          "type": "string",
          "rules": ["phone"],
          "isRequired": true,
          "description": "Телефон пользователя"
        },
        "code": {
          "type": "string",
          "isRequired": true,
          "description": "Пароль"
        }
      },
      "example": {
        "phone": "79046473638",
        "code": "1234"
      }
    },
    "response": {
      "params": {
        "access_token": {
          "type": "string",
          "isNullable": false,
          "description": "Токен для отправки в HTTP-заголовке Authorization"
        },
        "expires_in": {
          "type": "integer",
          "isNullable": false,
          "description": "Время жизни токена в секундах"
        },
        "token_type": {
          "type": "string",
          "isNullable": false,
          "description": "Тип токена"
        },
        "refresh_token": {
          "type": "string",
          "isNullable": false,
          "description": "Токен для обновления access-токена"
        }
      },
      "example": {
        "access_token": "ad29cf10a40ec04dae0e4e5f4cab2772df1b4734",
        "expires_in": 3600,
        "token_type": "Bearer",
        "refresh_token": "cc330ca2a44effb705f67152dbd67077dba3849e"
      }
    }
  },
  "testSuites": [
    {
      "request": {
        "phone": "79046473638",
        "code": "1234"
      },
      "response": {
        "result": {
          "access_token": "ad29cf10a40ec04dae0e4e5f4cab2772df1b4734",
          "expires_in": 3600,
          "token_type": "Bearer",
          "refresh_token": "cc330ca2a44effb705f67152dbd67077dba3849e"
        }
      }
    },
    {
      "request": {
        "phone": "asdasdasd",
        "code": "1234"
      },
      "response": {
        "error": {
          "code": -32600,
          "message": "Invalid Request"
        }
      }
    }
  ]
}